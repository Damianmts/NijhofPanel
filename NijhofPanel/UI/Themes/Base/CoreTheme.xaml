<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Main Theme -->
    <SolidColorBrush x:Key="PrimaryColor" Color="#046434"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#f5d105"/>
    
    <!-- Light Theme -->
    <SolidColorBrush x:Key="BackgroundColor" Color="White" />
    <SolidColorBrush x:Key="TextColor" Color="Black" />
    <SolidColorBrush x:Key="SidebarIconColor" Color="#046434"/>

    <!-- Dark Theme -->
    <SolidColorBrush x:Key="BackgroundColor_Dark" Color="#3b4453" />
    <SolidColorBrush x:Key="TextColor_Dark" Color="White" />
    <SolidColorBrush x:Key="SidebarIconColor_Dark" Color="#046434" />
</ResourceDictionary>